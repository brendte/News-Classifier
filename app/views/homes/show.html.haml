%h4 My Articles
= link_to 'All Articles', articles_path


= paginate @articles

%table
  %tr
    %th Title
    %th Summary
    %th Url
    %th Publication Date
    %th
    %th
    %th

  - @articles.each do |article|
    %tr
      %td= article.feed_entry.title
      %td= article.feed_entry.summary
      %td= link_to(article.feed_entry.url, article.feed_entry.url)
      %td= article.feed_entry.published_at

%br
  = link_to 'My Queries', queries_path

